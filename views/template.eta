<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Login</title>
    <link rel="stylesheet" href="/css/main.css">
</head>
<body>
<main>
    <section>
        <h1><%= it.title %></h1>
        <br>
        <% Object.keys(it.payload).forEach(function(prop) { %>
        <div><%= prop %> : <%= it.payload[prop] %></div>
        <% }) %>
    </section>
</main>
</body>
</html>